package customconf

environments {
    development {
        // conversations expire in 2 minutes
        conversationExpiracy = 1000 * 60 * 2
        chat.cronExpression = '0 0 0-23 ? * *'
        reminder.cronExpression = '0 0 * ? * *'
        projectFollowUp.cronExpression = '0 0 9 ? * FRI'
        knowledgeHeadsUp.cronExpression = '0 0 9 ? * MON'

        jabber.host = 'gmail.com'
        jabber.username = 'your_jabber_user'
        jabber.password = 'your_jabber_password'
        jabber.firstname = 'Project'
        jabber.lastname = 'Guide'
        jabber.status = 'Tracking Hours'
        jabber.organization = 'Your Organization'
        jabber.avatar = 'biohazard.jpg'
        twitter.username = 'some_twitter_user'
        twitter.password = 'your_twitter_password'
    }
}
